{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "GetProductOperationConfigSchema.json",
	"title": "GetProductOperationConfig",
	"description": "Input config file structure for the package uploader Get Product Operation",
	"type": "object",
	"properties": {
		"operationName": {
			"type": "string",
			"const": "GetProduct"
		},
		"productId": {
			"type": "string"
		},
		"bigId": {
			"type": "string"
		}
	},
	"oneOf": [
		{
			"required": [
				"operationName",
				"productId"
			]
		},
		{
			"required": [
				"operationName",
				"bigId"
			]
		}
	]
}